{
    "version":2,
    "builds": [
        {
            "src": "StockMarket/wsgi.py",
            "use": "@vercel/python",
            "config":{ "maxLambdasize": "15mb", "runtime": "python3.10.5"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static_build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "StockMarket/wsgi.py"
        }
    ]
}
